
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .property_agent_image {
    width: 30px;
    height: 30px;
    float: left;
    background-size: cover;
    margin-right: 10px;
    border-radius: 100%;
}
    .property_listing a {
    text-decoration: none;
}
</style>
<div class="row" ng-controller="ListRoomController">
    <div class="col-lg-9 col-md-9">
        <div class="row" style="margin-top:30px;">
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4 no-touch" ng-repeat="item in data.lstRoom">
                <a href="#" class="card" id="card-309">
                    <div class="figure">
                        <div class="featured-label">
                            <div class="featured-label-left"></div>
                            <div class="featured-label-content"><span class="fa fa-star"></span></div>
                            <div class="featured-label-right"></div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="img" style="background-image:url(http://localhost:6568/Content/images/{{item.Image}});"></div>
                        <div class="figCaption">
                            <div><span class="fa fa-home" style="margin-right:10px"></span>{{item.RoomTypeName}}</div>
                            <span><span class="fa fa-eye"></span> 9999+</span>
                            <span><span class="fa fa-heart-o"></span> 2</span>
                        </div>
                        <div class="figView"><span class="fa fa-eye"></span></div>
                        <div class="figType">{{item.Price|number}} Đ/tháng</div>
                    </div>
                    <h2>{{item.RoomName}}</h2>
                    <div class="cardAddress">
                        <span class="fa fa-map-marker" style="margin-right:10px;"></span>{{item.Address}}
                    </div>
                    <ul class="cardFeat">
                        <li><span class="fa fa-bed"></span><span style="margin-left:5px;">2</span></li>
                        <li><span class="fa fa-info"></span><span style="margin-left:5px;">{{item.Acreage|number}} m<sup>2</sup></span></li>
                        <li></li>
                        <li><span class="fa fa-male" style="margin-right:10px;"></span>5</li>
                        @*<li>
                            <span class="fa fa-user"></span>
                            <span>
                               {{item.FullName}}
                            </span>
                            <span>
                               {{item.FullName}}
                            </span>
                        </li>*@
                    </ul>
                    <div class="clearfix"></div>
                </a>
            </div>
        </div>
    </div>
    <div class="col-md-3 sidebar">
