        overflow: hidden;
        float: right;
        font-size: 12px;
    }

    .user_info_upload {
        background: url(https://sharecode.vn/Image/ic_cloud.png) 0 5px no-repeat;
        padding-left: 20px;
        height: 20px;
        overflow: hidden;
        float: left;
    }

    .user_margin40 {
        margin-right: 40px;
    }

    .user_info_upload span {
        color: #ff8401;
    }

    .user_info_date {
        background: url(https://sharecode.vn/Image/ic_date.png) 0 3px no-repeat;
        padding-left: 20px;
        height: 20px;
        overflow: hidden;
        float: left;
    }

        .user_info_date span {
            color: #ff8401;
        }

    .point_box {
        height: 50px;
        width: 15%;
        border: 1px solid #ff8401;
        border-radius: 3px;
        text-align: center;
        color: #ff8401;
        float: right;
        font-size: 14px;
    }

    .point_value {
        font-weight: bold;
        font-size: 18px;
        color: white;
        padding-right: 40px;
        height: 28px;
        padding-top: 4px;
        background: url(https://sharecode.vn/Image/ic_point2.png) #ff8401;
        background-position: right 0px top 0px;
        background-repeat: no-repeat;
    }
</style>
